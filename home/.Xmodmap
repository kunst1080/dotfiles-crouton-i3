! Search key is Control
! Control is Super
remove lock = Caps_Lock
remove control = Control_L
remove control = Control_R

! CapsLock
keycode 66 = Control_L
! Windows Key
keycode 133 = Control_L
! Left Ctrl
keycode 37 = Super_L
! Right Ctrl
keycode 105 = Super_R

add control = Control_L
!pointer = 1 2 3 5 4 6 7 8 9 10 11 12
